<form action="/Patologia/cambiar" method="POST">
  <head>
    <title>Crear Historia | MedicType</title>

  </head>
  <body>
  <br>




  <div class="container">

    <div class="row">
      <form class="col s12 m12 l12">
        <div class="row">

          <div class="col s12 m12 l12">
            <h4 class="flow-text center teal-text text-accent-4">Editar Patologia</h4>
          </div>

          <div class="input-field col s12 m6 l6">
            <input name="nombre" id="textarea1" type="tel" class="validate">
            <label for="icon_telephone">Nombre</label>
          </div>

          <div class="input-field col s12 m6 l6">
            <input name="patogenesis" id="icon_telephone" type="tel" class="validate">
            <label for="icon_telephone">Patogénesis</label>
          </div>

          <div class="input-field col s12 m6 l6">
            <textarea name="descripcion" id="textarea1" class="materialize-textarea" data-length="120"></textarea>
            <label for="textarea1">Descripción</label>
          </div>


          <div class="input-field col s12 m6 l6">
            <textarea name="transmision" id="textarea1" class="materialize-textarea" data-length="120"></textarea>
            <label for="textarea1">Transmisión</label>
          </div>

          <div class="input-field col s12 m6 l12">
            <input name="id" id="icon_telephone" type="hidden" value="<%= patologia.id %>">
          </div>

        <center>
          <input value="Enviar" name="action" class="btn waves-effect waves-light teal accent-4" type="submit"> </input>
        </center>


        </div>
      </form>
    </div>


  </div>
<br><br>


  <!--  Scripts-->
  <script>
    $(document).ready(function() {
      $('select').material_select();

      $('.datepicker').pickadate({
        selectMonths: true, // Creates a dropdown to control month
        selectYears: 200, // Creates a dropdown of 15 years to control year,
        today: 'Hoy',
        clear: 'Limpiar',
        close: 'Listo',
        closeOnSelect: false // Close upon selecting a date,
      });
    });
  </script>
  <script type="text/javascript">
    var Paciente;
    function registrarPaciente(){
      Paciente = {
        name: document.getElementById("first_name").value,
        apellido: document.getElementById("last_name").value,
        cedula: document.getElementById("cedula").value,
        correo: document.getElementById("email").value,
        bday: document.getElementById("bday").value,
        nacionalidad: document.getElementById("nacionalidad").value,
        nivelEducativo: document.getElementById("nivelEducativo").value,
        genero: document.getElementById("genero").value,
        edoCivil: document.getElementById("edoCivil").value,
        estado: document.getElementById("estado").value,
        direccion: document.getElementById("direccion").value,
      };
      console.log("Datos en obj Paciente.");
      //Querys aqui palante ToDo...
    }

  </script>

  </body>
  </html>
</form>
